		"main": [
			"com.llamalibrarian.LLaMaLibrarian"
		],
		"client": [
			"com.llamalibrarian.LLaMaLibrarianClient"
		]
	},
	"mixins": [
		"llamalibrarian.mixins.json",
		{
			"config": "llamalibrarian.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
