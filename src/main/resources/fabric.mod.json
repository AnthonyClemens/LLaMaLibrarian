	"name": "LLaMaLibrarian",
	"description": "Join forces with LlamaLibrarian, where you can engage in witty banter, explore new ideas, and build creative connections with your chatty llama friend!",
	"authors": [
		"Anthony Clemens aka billybobpickle"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/llamalibrarian/icon.png",
